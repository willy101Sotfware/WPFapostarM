<Application x:Class="WPFapostarM.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WPFapostarM"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <!-- Backgrounds -->
                    <BitmapImage x:Key="BgApostar" UriSource="/Images/Backgrounds/BgApostar.png" />
                    <BitmapImage x:Key="BgAsiva" UriSource="/Images/Backgrounds/BgAsiva.png" />
                    <BitmapImage x:Key="BgCalifica" UriSource="/Images/Backgrounds/BgCalifica.png" />
                    <BitmapImage x:Key="BgCalificacion" UriSource="/Images/Backgrounds/BgCalificacion.png" />
                    <BitmapImage x:Key="BgData" UriSource="/Images/Backgrounds/BgData.png" />
                    <BitmapImage x:Key="BgDia" UriSource="/Images/Backgrounds/BgDia.png" />
                    <BitmapImage x:Key="BgDigitaDinero" UriSource="/Images/Backgrounds/BgDigitaDinero.png" />
                    <BitmapImage x:Key="BgDigitaReferencia" UriSource="/Images/Backgrounds/BgDigitaReferencia.png" />
                    <BitmapImage x:Key="BgGray" UriSource="/Images/Backgrounds/BgGray.png" />
                    <BitmapImage x:Key="BgPay" UriSource="/Images/Backgrounds/BgPay.png" />
                    <BitmapImage x:Key="BgReferencia" UriSource="/Images/Backgrounds/BgReferencia.png" />
                    <BitmapImage x:Key="BgSmall" UriSource="/Images/Backgrounds/BgSmall.png" />
